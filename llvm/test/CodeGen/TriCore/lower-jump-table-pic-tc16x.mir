# RUN: llc -O0 -mtriple=tricore -mattr=-tc18 -global-isel -start-after=tricore-jump-tables --relocation-model=pic -verify-machineinstrs -show-mc-encoding %s -o - | FileCheck %s

# CHECK-LABEL:   .text
# CHECK:         .file  "lower-jump-table-pic-tc16x.mir"
# CHECK:         .globl  jt_test                # -- Begin function jt_test
# CHECK:         .type  jt_test,@function
# CHECK-LABEL: jt_test:                         # @jt_test
